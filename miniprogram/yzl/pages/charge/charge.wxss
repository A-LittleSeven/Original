page {
	background-color: #f3f7fa;
}

.vertical-center {
	display: flex;
	display: -webkit-flex;
	align-items: center;
}

.transform-center {
	position: relative;
	top: 50%;
	transform: translateY(-50%);
}

.mgr-40 {
	margin-right: 40rpx;
}

.balance-view {
	height: 160rpx;
	width: 100%;
	background-color: white;
}

.font-26 {
	font-size: 26rpx;
}

.color-3e {
	color: #3e3e3e;
}

.font-43 {
	font-size: 43rpx;
}

.color-ff43 {
	color: #ff435e;
}

.text-center {
	text-align: center;
}

.collection-wrapper {
	margin-top: 200rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
}

.list {
	display: flex;
	flex-flow: row wrap;
	justify-content: space-between;
}

.cell-content {
	margin-bottom: 20rpx;
	width: 227rpx;
	height: 103rpx;
	background-color: white;
	text-align: center;
	border-radius: 15rpx;
}

.title {
	display: block;
	font-size: 32rpx;
	color: #535963;
	margin-top: 10rpx;
}

.des {
	display: block;
	font-size: 26rpx;
	color: #bbc1cc;
}

.activeli {
	border: 1rpx solid #00dbf5;
}

.color-active {
	color: #00dbf5;
}

.cell {
	width: 100%;
	height: 100%;
	border-radius: 15rpx;
}

.input-wrap {
	margin-top: 48rpx;
	margin-left: 30rpx;
	margin-right: 30rpx;
	background-color: white;
}

.btn-wrap {
	margin-top: 60rpx;
	margin-left: 30rpx;
	margin-right: 30rpx;
}

