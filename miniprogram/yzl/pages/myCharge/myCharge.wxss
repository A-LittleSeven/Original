page {
	background-color: #f0f0f0;
}

.vertical-center {
	display: flex;
	display: -webkit-flex;
	align-items: center;
}

.horizon-center {
	margin: 0 auto;
}

.transform-center {
	position: relative;
	top: 50%;
	transform: translateY(-50%);
}

.header {
	text-align: center;
	width: 100%;
}

.separate {
	width: 100%;
	height: 1rpx;
}

.bg-f6 {
	background-color: #f6f6f6;
}

.w-80 {
	width: 80%;
	margin-left: 10%;
}

.mgt-20 {
	margin-top: 20rpx;
}

.charge-wrap {
	width: 204rpx;
	height: 204rpx;
	background-color: #ff9901;
	border-radius: 50%;
	margin-top: 29rpx;
}

.charge-title {
	font-size: 34rpx;
	display: block;
	margin-bottom: 13rpx;
}

.charge-value {
	font-size: 40rpx;
}

.color-white {
	color: #ffffff;
}

